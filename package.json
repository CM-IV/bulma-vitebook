{
  "name": "bulma-ui",
  "description": "A UI Library for Bulma CSS",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "vitebook:dev": "vitebook dev",
    "vitebook:build": "vitebook build",
    "vitebook:preview": "vitebook preview",
    "test": "vitest run",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx . --ignore-path .gitignore",
    "format": "run-s format:*",
    "format:eslint": "npm run lint:eslint -- --fix",
    "lint:prettier": "prettier . --check --ignore-path .gitignore --loglevel warn",
    "format:prettier": "npm run lint:prettier -- --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "preact": "^10.7.1",
    "preact-render-to-string": "^5.1.19",
    "wouter-preact": "^2.8.0-alpha.2"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.1.7",
    "@testing-library/preact": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitebook/client": "^0.23.5",
    "@vitebook/core": "^0.23.5",
    "@vitebook/markdown-preact": "^0.23.5",
    "@vitebook/markdown-shiki": "^0.23.5",
    "@vitebook/preact": "^0.23.5",
    "@vitebook/theme-default": "^0.23.5",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "happy-dom": "^2.55.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.0.0",
    "prettier": "^2.0.0",
    "typescript": "^4.4.0",
    "vite": "2.8.6",
    "vitest": "^0.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,md,json}": "prettier --write"
  }
}
